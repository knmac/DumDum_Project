	}

	public Button(Bitmap[] images, Point position) {
		super(images, position);
		// TODO Auto-generated constructor stub
	}

	public Button(Bitmap image, Point position) {
		super(image, position);
		// TODO Auto-generated constructor stub
	}

	public Button() {
		// TODO Auto-generated constructor stub
	}

}
